{
  "manifest_version": 2,
  "name": "My Chrome Extension",
  "description": "My chrome extension that accesses Gmail data.",
  "version": "1.0",
  "permissions": [
    "https://mail.google.com/*",
    "activeTab",
    "storage"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html"
  }
}
